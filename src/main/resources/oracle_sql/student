CREATE TABLE Student (
 id   VARCHAR2(20) PRIMARY KEY 
,name VARCHAR2(100) 
,sex  CHAR(10)
,age  INT 
);
COMMENT ON  TABLE Student      IS '学生表';
COMMENT ON COLUMN Student.id   IS '学号' ;
COMMENT ON COLUMN Student.name IS '学生姓名' ;
COMMENT ON COLUMN Student.sex  IS '性别 01 男  02 女' ;
COMMENT ON COLUMN Student.age  IS '年龄' ;



INSERT INTO Student values('001','小明','01',11);
COMMIT;